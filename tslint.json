{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "jsRules": {
    "quotemark": false,
    "no-submodule-imports": false
  },
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "no-console": false
  },
  "rulesDirectory": []
}
